{
  "version": 2,
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PHP_VERSION": "8.2"
  },
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php"
    },
    {
      "src": "admin/*.php",
      "use": "vercel-php"
    }
  ]
}