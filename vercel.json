{
  "version": 2,
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "admin/*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PHP_VERSION": "8.3"
  }
}